{
  "name": "errlog-analytics-frontend",
  "version": "1.0.0",
  "description": "Frontend for Error Log Analytics - Vue.js 3 + Quasar",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"src/**/*.{vue,js,ts,jsx,tsx,css,scss,json}\""
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.9.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/ui": "^0.34.6",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  }
}
